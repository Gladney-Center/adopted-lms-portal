{
  "hosting": {
    "public": "portal",
    "redirects": [],
    "rewrites": [
      {
        "source": "**/**",
        "function": "portal"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "source": "pFunctions"
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "emulators": {
    "firestore": {
      "port": "PORT"
    }
  }
}